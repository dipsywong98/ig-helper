{
  "compilerOptions": {
    "outDir": "dist",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "types": [
      "jest",
      "node",
      "@testing-library/jest-dom"
    ],
    "jsx": "react-jsx",
    "target": "es2020",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "noFallthroughCasesInSwitch": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true
  },
  "include": [
    "src",
    "test",
    "vite.config.ts",
    "coverage/realtime.example.ts"
  ]
}